   "outputs": [],
   "source": [
    "from sklearn.impute import SimpleImputer\n",
    "\n",
    "# Assuming df is your DataFrame\n",
    "\n",
    "# 1. Handle missing values for numerical columns with mean\n",
    "num_imputer = SimpleImputer(strategy='mean')\n",
    "\n",
    "heart_df['trestbps'] = num_imputer.fit_transform(heart_df[['trestbps']])\n",
    "heart_df['chol'] = num_imputer.fit_transform(heart_df[['chol']])\n",
    "heart_df['thalch'] = num_imputer.fit_transform(heart_df[['thalch']])\n",
    "heart_df['oldpeak'] = num_imputer.fit_transform(heart_df[['oldpeak']])\n",
    "heart_df['ca'] = num_imputer.fit_transform(heart_df[['ca']])\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "id": "e4d66203-1b05-436b-9f00-67cb049d98d3",
   "metadata": {},
   "outputs": [],
   "source": [
    "# 2. Handle missing values for categorical columns with most frequent\n",
    "cat_imputer = SimpleImputer(strategy='most_frequent')\n",
    "\n",
    "# Impute missing values and flatten the result\n",
    "heart_df['fbs'] = cat_imputer.fit_transform(heart_df[['fbs']]).ravel()\n",
    "heart_df['restecg'] = cat_imputer.fit_transform(heart_df[['restecg']]).ravel()\n",
    "heart_df['exang'] = cat_imputer.fit_transform(heart_df[['exang']]).ravel()\n",
    "heart_df['slope'] = cat_imputer.fit_transform(heart_df[['slope']]).ravel()\n",
    "heart_df['thal'] = cat_imputer.fit_transform(heart_df[['thal']]).ravel()\n"
   ]
  },
  {
