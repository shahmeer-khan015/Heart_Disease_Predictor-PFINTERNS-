   "source": [
    "# 6. Convert slope column\n",
    "slope_mapping = {\n",
    "    'upsloping': 0,\n",
    "    'flat': 1,\n",
    "    'downsloping': 2\n",
    "}\n",
    "heart_df['slope'] = heart_df['slope'].map(slope_mapping)"
