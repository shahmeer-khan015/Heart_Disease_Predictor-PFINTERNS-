   "source": [
    "# 1. Convert sex column\n",
    "heart_df['sex'] = heart_df['sex'].map({'Male': 0, 'Female': 1})"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 11,
   "id": "96436a2f-354e-4786-bb0e-33f803ae271e",
   "metadata": {},
   "outputs": [],
   "source": [
    "# 2. Convert cp column\n",
    "cp_mapping = {\n",
    "    'typical angina': 0,\n",
    "    'atypical angina': 1,\n",
    "    'non-anginal': 2,\n",
    "    'asymptomatic': 3\n",
    "}\n",
    "heart_df['cp'] = heart_df['cp'].map(cp_mapping)\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 13,
   "id": "688bd11a-861e-468e-9b01-c02482edcf5e",
   "metadata": {},
   "outputs": [],
   "source": [
    "# 3. Convert fbs column\n",
    "heart_df['fbs'] = heart_df['fbs'].astype(int)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 22,
   "id": "525e25de-8b31-4a49-87f3-8a643f755b2d",
   "metadata": {},
   "outputs": [],
   "source": [
    "# 4. Convert restecg column\n",
    "restecg_mapping = {\n",
    "    'normal': 0,\n",
    "    'lv hypertrophy': 2,\n",
    "    'having ST-T wave abnormality': 1\n",
    "}\n",
    "heart_df['restecg'] = heart_df['restecg'].map(restecg_mapping)\n"
   ]
  },
